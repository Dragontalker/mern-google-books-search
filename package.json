  "bugs": {
    "url": "https://github.com/Dragontalker/mern-google-books-search/issues"
  },
  "homepage": "https://github.com/Dragontalker/mern-google-books-search#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
